{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/notes/(.*)",
      "dest": "/api/notes/[...path].js"
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth/[...path].js"
    }
  ],
  "env": {
    "ATLAS_URI": "@atlas_uri",
    "AUTH0_DOMAIN": "@auth0_domain",
    "AUTH0_CLIENT_ID": "@auth0_client_id",
    "AUTH0_CLIENT_SECRET": "@auth0_client_secret"
  }
}